crossing_points <- function(sine1, sine2){
estados = c()
for (i in 1:length(sine1)){
if (sine1[i] > sine2[i]) {
estado = 0
} else {
estado = 1
}
estados[[i]] <- estado
}
crossings = c()
for (i in 2:length(estados)) {
if (estados[i] != estados[i-1]) {
crossings[[i]] = i
}
}
crossings = na.omit(crossings)
return(crossings)
}
